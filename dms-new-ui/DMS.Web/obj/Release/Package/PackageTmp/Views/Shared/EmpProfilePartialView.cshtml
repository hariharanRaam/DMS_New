                                @*<input type="text" name="Pin" placeholder="Pin Code" class="form-control" id="Pin" value="@ViewBag.Pin">*@

                            </div>

                        </div>


                    </div>
                    <div class="form-horizontal">
                        <div class="form-group">
                            <div class="col-lg-6  form-inline">

                                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <label>State Name &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;:  &nbsp;</label>
                                <span class="minwith">@ViewBag.State</span>
                                @*<input type="text" name="Address" placeholder="Address" class="form-control" id="Address" value="@ViewBag.Address">*@

                            </div>
                            <div class="col-lg-6 form-inline">

                                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;  <label>Region Name  &nbsp;  &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;: &nbsp;</label>
                                <span class="minwith">@ViewBag.Region </span>
                                @*<input type="text" name="Pin" placeholder="Pin Code" class="form-control" id="Pin" value="@ViewBag.Pin">*@

                            </div>

                        </div>


                    </div>
                    <div class="form-horizontal">
                        <div class="form-group">
                            <div class="col-lg-6  form-inline">

                                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <label> Mobile No &nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp; &nbsp; &nbsp;:  &nbsp;</label>
                                <span class="minwith">@ViewBag.EmpMobileNo</span>
                                @*<input type="text" name="Address" placeholder="Address" class="form-control" id="Address" value="@ViewBag.Address">*@

                            </div>
                            <div class="col-lg-6 form-inline">

                                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <label>Email Id &nbsp; &nbsp; &nbsp;&nbsp; &nbsp;  &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;: &nbsp;</label>
                                <span class="minwith">@ViewBag.EmpMailId </span>
                                @*<input type="text" name="Pin" placeholder="Pin Code" class="form-control" id="Pin" value="@ViewBag.Pin">*@

                            </div>

                        </div>

                    </div>

                </div>
            </div>
        </div>
    </div>
</body>
</html>