                                @*<input type="text" name="Address" placeholder="Address" class="form-control" id="Address" value="@ViewBag.Address">*@

                            </div>
                            <div class="col-lg-6 form-inline">

                                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;  <label>Pin Code  &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;  &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;: &nbsp;</label>
