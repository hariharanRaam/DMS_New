    function attach() {
        debugger;
        var remarks = "";
        var activeflag = $("input[name='active_flag']:checked").val();
        remarks = $("#edremarks").val();
        var filesize = $('#File').val();
        if (filesize == "" || filesize == null) {
            $.alert({
                title: 'DMS',
                content: 'Choose File.!',
                type: 'blue',
            });
            return false;
        } else if (remarks == null || remarks == "undefined" || remarks == "") {
            $.alert({
                title: 'DMS',
                content: 'Remarks Cannot be Empty',
                type: 'blue',
            });
            return false;
        }

        var Filsize = $('#File')[0].files[0].size;

        if (Filsize > 5242880) {
            $.alert({
                title: 'DMS',
                content: 'File size should be below in 5 MB.!',
                type: 'blue',
            });
            return false;
        }

        var grid = $("#grid").data("kendoGrid");
        var row = $("input:checked", grid.tbody).closest("tr");
        var item = grid.dataItem(row);
        if (item == null) {
            $.alert({
                title: 'DMS',
                content: 'Select preview button in grid.!',
                type: 'blue',
            });
            return false;
        }

        var attached = item.entries[0]
        var fileData = new FormData();
        var fileUpload = $("#File").get(0);
        var files = fileUpload.files;
        // Looping over all files and add it to FormData object
        for (var i = 0; i < files.length; i++) {
            fileData.append(files[i].name, files[i]);
        }

        fileData.append('attacheid', attached);
        fileData.append('remarks', remarks);
        fileData.append('activeflag', activeflag);

        $.ajax({
            url: '@Url.Action("InterFiling", "InterFiling")',
            type: "POST",
            contentType: false,
            processData: false,
          //  datatype: "json",
           // data: { 'parentcode': master, 'dependcode': mastercode },
            data: fileData,
            success: function (response) {
                debugger;
                $.alert({
                    title: 'DMS',
                    content: 'File attached successfully.!',
                    type: 'green',
                });
                window.location.href = '../InterFiling/InterFilingNew';
                //return false;
            },
            error: function (error) {
                $.alert({
                    title: 'DMS',
                    content: 'Error.!',
                    type: 'red',
                });
            }
        });
    }
