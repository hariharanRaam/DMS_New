        for (var i = 0; i < attributes5.length - 1; i++) {
            if (sorted_arr[i + 1] == sorted_arr[i]) {
                results.push(sorted_arr[i]);
                duplicate = "true";
            }
        }

        var uniqueNames = [];
        $.each(results, function (i, el) {
            if ($.inArray(el, uniqueNames) === -1) uniqueNames.push(el);
