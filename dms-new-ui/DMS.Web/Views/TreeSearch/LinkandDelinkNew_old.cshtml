    function forbulkMail() {
        debugger;
        $("#hdn_mailtype").val("");
        $("#hdn_mailtype").val("multiple");

        var grid = $("#grid").data("kendoGrid");
        var checkedrows = $("#attributecheckbox:checked", grid.tbody).closest("tr");
